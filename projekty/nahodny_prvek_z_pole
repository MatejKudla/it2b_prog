#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    // Definice pole
        int pole[] = {10, 20, 30, 40, 50};

    // Pevně daná velikost pole
        int velikost = 5;

    // Inicializace generátoru náhodných čísel
        srand(time(NULL));

    // Náhodný výběr indexu
        int nahodnyIndex = rand() % velikost;

    // Výpis náhodně vybraného prvku
        printf("Nahodne vybrany prvek z pole: %d\n", pole[nahodnyIndex]);

    return 0;
}